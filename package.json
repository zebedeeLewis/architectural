{
  "name": "architectural",
  "version": "1.0.0",
  "description": "Architectural website template.",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./build/config/webpack.prod.js",
    "build:stats": "cross-env NODE_ENV=production webpack --profile --json=compilation-stats.json --config ./build/config/webpack.prod.js",
    "start": "cross-env NODE_ENV=development webpack serve --config ./build/config/webpack.dev.js",
    "inspect:webpack:prod": "cross-env NODE_ENV=production node-nightly --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./build/config/webpack.prod.js",
    "inspect:webpack:dev": "cross-env NODE_ENV=development node-nightly --inspect-brk ./node_modules/webpack/bin/webpack.js serve --config ./build/config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebedeeLewis/architectural.git"
  },
  "author": "zebedee Lewis <zebedeeslewis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zebedeeLewis/architectural/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "apply-loader": "^2.0.0",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "directory-tree": "^2.2.5",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "leaflet": "^1.7.1",
    "loader-runner": "^4.1.0",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.1",
    "node-nightly": "^1.7.3",
    "postcss": "^8.1.8",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "prettier-loader": "^3.3.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "val-loader": "^2.1.2",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@splidejs/splide": "^2.4.14",
    "normalize.css": "^8.0.1"
  }
}
