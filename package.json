{
  "name": "architectural",
  "version": "1.0.0",
  "description": "Architectural website template.",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./build/config/webpack.prod.js",
    "build:stats": "cross-env NODE_ENV=production webpack --profile --json=compilation-stats.json --config ./build/config/webpack.prod.js",
    "start": "cross-env NODE_ENV=development webpack serve --config ./build/config/webpack.dev.js",
    "inspect:webpack:prod": "cross-env NODE_ENV=production node-nightly --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./build/config/webpack.prod.js",
    "inspect:webpack:dev": "cross-env NODE_ENV=development node-nightly --inspect-brk ./node_modules/webpack/bin/webpack.js serve --config ./build/config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebedeeLewis/architectural.git"
  },
  "author": "zebedee Lewis <zebedeeslewis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zebedeeLewis/architectural/issues"
  },
  "homepage": "",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@splidejs/splide": "^4.1.4",
    "apply-loader": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.2",
    "directory-tree": "^3.5.2",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.8",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "leaflet": "^1.9.4",
    "loader-runner": "^4.3.1",
    "mini-css-extract-plugin": "^2.9.4",
    "node-nightly": "^1.8.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.6.2",
    "prettier-loader": "^3.3.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.93.3",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "val-loader": "^6.0.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
